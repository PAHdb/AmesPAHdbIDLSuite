<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN"
 "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">

<!-- Generated by IDLdoc [% idldoc_version %] on [% date %] -->

<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" lang="en">
  <head>
    <meta http-equiv="Content-Type" content="text/html;charset=[% charset %]"/>
    <title>[% title %]</title>

    <script type="text/javascript" src="idldoc-resources/js/jquery.js"></script>
    <script type="text/javascript" src="idldoc-resources/js/copybutton.js"></script>

    [% IF use_latex %]
      <script type="text/javascript"
  src="[% relative_root %]idldoc-resources/mathjax/MathJax.js?config=default">
      </script>
    [% END %]

    [% IF embed %]
    <style type="text/css" media="all">
      [% INCLUDE css_location %]
    </style>
    <style type="text/css" media="print">
      [% INCLUDE print_css_location %]
    </style>
    [% ELSE %]
    <link rel="stylesheet" type="text/css" media="all"
          href="idldoc-resources/main.css" />
    <link rel="stylesheet" type="text/css" media="print"
          href="idldoc-resources/main-print.css" />
    [% END %]

    <script type="text/javascript">
      function setTitle() {
        parent.document.title="[% title %]";
      }
    </script>
  </head>

  <body onload="setTitle();">
    <div class="containter">

      [% INCLUDE_TEMPLATE idldoc_header_location %]

      <div class="content">

      [% IF has_overview_comments %]
      <h2>Overview</h2>
[% overview_comments %]
      [% END %]

      [% IF ~just_files %]
      <h2 id="directories">Directories</h2>

      <table class="attribute box small" summary="Directories">
        [% IF n_visible_dirs gt 0 %]
        [% FOREACH d IN visible_dirs %][% SCOPE d %]
        <tr>
          <td class="name"><a href="[% url %]dir-overview.html">[% location %]</a></td>
          <td>[% system_overview_comments %]</td>
        </tr>
        [% END %][% END %]
        [% END %]
      </table>
      [% END %]

      [% IF has_author_info %]
      <h2>Author information</h2>

        <dl class="attribute small" summary="Library author information">
          [% IF has_author %]
            <dt>Author:</dt>
            <dd>[% author %]</dd>
          [% END %]

          [% IF has_copyright %]
            <dt>Copyright:</dt>
            <dd>[% copyright %]</dd>
          [% END %]

          [% IF has_history %]
            <dt>History:</dt>
            <dd>[% history %]</dd>
          [% END %]

          [% IF has_version %]
            <dt>Version:</dt>
            <dd>[% version %]</dd>
          [% END %]
        </dl>
      [% END %]


      <h2 id="statistics">Project statistics</h2>

      <table class="attribute small" summary="Project statistics">
        <tr>
          <td class="name">Directories:</td>
          <td>[% n_visible_dirs %]</td>
        </tr>

        <tr>
          <td class="name">.pro files:</td>
          <td>[% n_visible_pro_files %]</td>
        </tr>

        <tr>
          <td class="name">.sav files:</td>
          <td>[% n_sav_files %]</td>
        </tr>

        <tr>
          <td class="name">Routines:</td>
          <td>[% n_routines %]</td>
        </tr>

        <tr>
          <td class="name">Lines:</td>
          <td>[% n_lines %]</td>
        </tr>

        [% IF requires_version ne '' %]
        <tr>
          <td class="name">Required IDL version:</td>
          <td>[% requires_version %]</td>
        </tr>
        [% END %]
      </table>

      [% IF footer ne '' %][% INCLUDE_TEMPLATE footer %][% END %]
    </div>

    [% INCLUDE_TEMPLATE idldoc_footer_location %]
      </div>
    </body>
</html>
